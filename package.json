{
  "name": "weatherwear",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "remix vite:build",
    "db:migrate": "tsx ./db/migrate.ts",
    "dev": "node ./server.mjs",
    "drizzle-kit": "drizzle-kit",
