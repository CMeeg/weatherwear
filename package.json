{
  "name": "weatherwear",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "remix vite:build",
    "db:migrate:drop": "drizzle-kit drop",
    "db:migrate:gen": "drizzle-kit generate",
    "db:migrate:run": "tsx ./app/db/migrate.ts",
    "db:pull": "drizzle-kit introspect",
    "db:push": "drizzle-kit push",
    "db:seed": "tsx ./app/db/seed.ts",
    "dev": "node ./server/index.js",
    "drizzle-kit": "drizzle-kit",
    "env:init": "pwsh ./.azd/scripts/create-env-local.ps1",
    "lint": "run-p lint:* -c",
    "lint:css": "npx stylelint --cache --cache-location ./node_modules/.cache/stylelint \"**/*.css\"",
    "lint:js": "eslint --ignore-path .eslintignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "cross-env NODE_ENV=production node ./server/index.js",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@0dep/pino-applicationinsights": "^0.1.2",
    "@azure/storage-blob": "^12.18.0",
    "@microsoft/applicationinsights-react-js": "17.2.0",
