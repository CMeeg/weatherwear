    "supabase": "supabase",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@remix-run/express": "^2.4.1",
    "@remix-run/node": "^2.4.1",
    "@remix-run/react": "^2.4.1",
    "@remix-validated-form/with-zod": "^2.0.7",
    "@sindresorhus/slugify": "^2.2.1",
    "cross-env": "^7.0.3",
    "date-fns": "^3.2.0",
    "drizzle-orm": "^0.29.3",
    "express": "^4.18.2",
    "isbot": "^3.6.8",
    "openai": "^4.24.1",
    "postgres": "^3.4.3",
    "react": "^18.2.0",
    "react-aria-components": "^1.0.0",
    "react-dom": "^18.2.0",
    "remix-validated-form": "^5.1.5",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
